class Solution {
    public int numIdenticalPairs(int[] nums) {
        int num =0;
        for(int i =0; i<nums.length; i++)
        {
            for(int j = 0; j<nums.length; j++)
            {
                if(i<j)
                {
                    if(nums[i] == nums[j])
                        num++;
                }
            }
        }
        return num;
    }
}
